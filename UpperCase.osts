{"version":"0.3.0","body":"function main(workbook: ExcelScript.Workbook) {\n    // Get the active worksheet\n    const selectedSheet = workbook.getActiveWorksheet();\n    \n    // Pause sheet protection\n    selectedSheet.getProtection().pauseProtection(\"imsorrydave\");\n\n    // Define ranges to convert to uppercase\n    const rangesToUppercase = [\n        selectedSheet.getRange(\"B7:E38\"),\n        selectedSheet.getRange(\"G7:G38\")\n    ];\n\n    // Convert values to uppercase for each range\n    rangesToUppercase.forEach(range => {\n        const values = range.getValues();\n        const upperCaseValues = values.map(row => \n            row.map(cell => typeof cell === 'string' ? cell.toUpperCase() : cell)\n        );\n        range.setValues(upperCaseValues);\n    });\n\n    // Resume sheet protection\n    selectedSheet.getProtection().resumeProtection();\n}\n","description":"","noCodeMetadata":null,"parameterInfo":"{\"version\":1,\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}},\"signature\":{\"comment\":\"\",\"parameters\":[{\"name\":\"workbook\",\"comment\":\"\"}]}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}