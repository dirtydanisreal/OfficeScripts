{"version":"0.3.0","body":"function main(workbook: ExcelScript.Workbook) {\n    // Get the active worksheet\n    let sheet: ExcelScript.Worksheet = workbook.getActiveWorksheet();\n\n    // Get the value selected in the dropdown list in cell A1\n    let searchValue: string | number | boolean = sheet.getRange(\"A1\").getValue();\n\n    // Define the range to search in column A from A2 to A491\n    let searchRange: ExcelScript.Range = sheet.getRange(\"A2:A491\");\n\n    // Get all values in the search range\n    let cells: (string | number | boolean)[][] = searchRange.getValues();\n\n    // Initialize the variable for the found row\n    let foundRow: number = -1;\n\n    // Find the row containing the search value using a traditional for loop for faster exit\n    for (let i = 0; i < cells.length; i++) {\n        if (cells[i][0] === searchValue) {\n            foundRow = i + 2; // Add 2 because the range starts at A2\n            break; // Exit the loop as soon as the value is found\n        }\n    }\n\n    // If the cell is found, select the entire row\n    if (foundRow !== -1) {\n        sheet.getRange(`A${foundRow}`).getEntireRow().select(); // Select the entire row\n    } else {\n        // Optionally remove console logging for slightly improved performance\n        console.log(\"Value not found\");\n    }\n}\n","description":"","noCodeMetadata":"","parameterInfo":"{\"version\":1,\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}},\"signature\":{\"comment\":\"\",\"parameters\":[{\"name\":\"workbook\",\"comment\":\"\"}]}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}